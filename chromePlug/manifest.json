{

	"manifest_version": 2,

	"name": "demo",

	"version": "1.0.0",

	"description": "简单的Chrome扩展demo",

	"icons":
	{
		"16": "img/icon/icon.png",
		"48": "img/icon/icon.png",
		"128": "img/icon/icon.png"
	},

	"background":
	{
	
		"page": "background.html"
	},

	"browser_action": 
	{
		"default_icon": "img/icon/icon.png",
        "default_title": "这是一个示例Chrome插件",
        "default_popup": "popup.html"
    },
    "content_scripts": 
	[
		{
		
			"matches": ["https://codesign.woa.com/*"],
			"js": ["js/jquery-3.6.js", "js/jquery-ui.js",  "js/storageUtil.js", "js/utils.js", "js/content-message.js", "js/content-topmeun.js",
        "js/materials-data.js","js/content-materials.js", "js/content-drawframe.js", "js/content-script.js"],
			"css":["css/hz-content.css"],
			"run_at": "document_end"
		}
	],
	"permissions": [
		"activeTab",
		"storage"
	],
	"web_accessible_resources": [
		"img/*"
	  ]





}